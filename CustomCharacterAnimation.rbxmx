			<string name="ScriptGuid">{A407EEBD-AC51-41D5-A031-A5E1A63CC5C8}</string>
			<ProtectedString name="Source"><![CDATA[local CollectionService = game:GetService("CollectionService")
local NewThread = require(game.ServerScriptService.NewThread)
local States = {"FallingDown", "RunningNoPhysics", "Climbing", "Ragdoll", "GettingUp", "Flying", "Seated", "Swimming", "PlatformStanding"}

local function Animate(Char)
	local Hum = Char:WaitForChild("Humanoid")
	local Anims = {}
	
	for _,State in pairs(States) do
		Hum:SetStateEnabled(Enum.HumanoidStateType[State], false)
	end
	
	for _,Anim in pairs(script:GetChildren()) do
		Anims[Anim.Name] = Hum:LoadAnimation(Anim[math.random(1, #Anim:GetChildren())])
	end
	
	Hum.Running:Connect(function(Speed)
		if Speed ~= 0 then
			Anims.Idle:Stop()
			Anims.Run:Play()
		else
			Anims.Run:Stop()
			Anims.Idle:Play()
		end
	end)
	
	Hum.Jumping:Connect(function(Entering)
		if Entering then
			Anims.Jump:Play()
		else
			Anims.Jump:Stop()
		end
	end)
	
	Hum.FreeFalling:Connect(function(Entering)
		if Entering then
			Anims.FreeFall:Play()
		else
			Anims.FreeFall:Stop()
		end
	end)	
end

for _,Char in pairs(CollectionService:GetTagged("NPC")) do
	NewThread(Animate, Char)
end

CollectionService:GetInstanceAddedSignal("NPC"):Connect(function(Char)
	NewThread(Animate, Char)
end)]]></ProtectedString>
